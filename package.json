{
  "name": "scryfall-deckbuilder-helper",
  "private": true,
  "version": "0.0.1",
  "description": "A chrome extension for adding features to the Scryfall deckbuilder",
  "scripts": {
    "lint": "standard --verbose | snazzy",
    "posttest": "npm run lint",
    "test": "mocha --recursive --require @babel/register test/ ",
    "build": "node utils/build.js",
    "start": "node utils/webserver.js"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "2.1.0",
    "file-loader": "3.0.1",
    "fs-extra": "7.0.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "mocha": "^6.2.0",
    "sinon": "^7.4.1",
    "sinon-chai": "^3.3.0",
    "sinon-chrome": "^3.0.1",
    "snazzy": "^8.0.0",
    "standard": "^13.1.0",
    "style-loader": "^0.23.1",
    "webpack": "4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "3.1.14",
    "write-file-webpack-plugin": "4.5.0"
  },
  "dependencies": {
    "framebus": "^3.0.2",
    "scryfall-client": "^0.10.0"
  },
  "standard": {
    "globals": [
      "chai",
      "sinon",
      "describe",
      "it",
      "afterEach",
      "beforeEach",
      "chrome",
      "sandbox",
      "expect",
      "Scryfall",
      "ScryfallAPI"
    ]
